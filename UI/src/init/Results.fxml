<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane fx:id="gridPane" prefHeight="556.0" prefWidth="1324.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="init.ResultsController">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>
    <children>
        <ListView fx:id="simulations" prefHeight="300.0" prefWidth="300.0" GridPane.columnIndex="0" GridPane.rowIndex="0" GridPane.valignment="TOP" />
        <ComboBox fx:id="viewSelection" onAction="#changeView" prefWidth="150.0" promptText="Select View" GridPane.halignment="LEFT" GridPane.rowIndex="1" GridPane.valignment="TOP" />
      <Button mnemonicParsing="false" onAction="#runSimulationAgain" text="Run Simulation again" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="TOP" />
   </children>
</GridPane>
